{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "fscodegen",
            "command": "powershell",
            "args": [
                "./codegen.ps1 ${fileBasenameNoExtension} fscodegen"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "fsrun",
            "command": "powershell",
            "args": [
                "./codegen.ps1 ${fileBasenameNoExtension} fsrun"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
    ]
}